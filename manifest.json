{
  "name": "Ranobes",
  "description": "Extension for Ranobes - Seen novels",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "bookmarks"
  ],
  "content_scripts": [
    {
      "matches": ["*://ranobes.top/*"],
      "js": ["ranobes.js"],
      "run_at": "document_start"
    }
  ],
  "host_permissions": ["*://ranobes.net/*"]
}